@model SolucionCacao.Models.Climas
    
@{
    ViewData["Title"] = "Clima";
}

<div class="col-md-4">
    <div class="row">
        <div class="form-group">
            <label class="control-label"><b>Ciudad: </b></label>
            @Html.DisplayFor(model => model.Ciudad)
        </div>
        <div class="form-group">
            <label class="control-label"><b>Humedad: </b></label>
            @Html.DisplayFor(model => model.Humedad)<span>%</span>
        </div>
        <div class="form-group">
            <label class="control-label"><b>Velocidad del viento: </b></label>
            @Html.DisplayFor(model => model.Viento)<span> km/h</span>
        </div>
        <div class="form-group">
            <label class="control-label"><b>Temperatura: </b></label>
            @Html.DisplayFor(model => model.Sensacion)<span>°C</span>
        </div>
        <div class="form-group">
            <label class="control-label"><b>Probabilidad precipitación: </b></label>
            @Html.DisplayFor(model => model.Sensacion)<span>%</span>
        </div>
    </div>
</div>
